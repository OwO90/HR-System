#include <iostream>
using namespace std;
int main() {
    int reportType;
    string employees[] = {"Ali", "Sara", "Omar", "Lina", "Said"};
    cout << "\n\nSelect report type:\n1. List Employees\n2. Count Employees\n";
    cin >> reportType;
    switch (reportType) {
        case 1:
            cout << "\nEmployee Names:\n";
            for (int i = 0; i < sizeof(employees) / sizeof(employees[0]); i++) {
                cout << employees[i] << endl;
            }
            break;
        case 2:
            cout << "\nTotal Employees: ";
            cout << sizeof(employees) / sizeof(employees[0]) << endl;
            break;
        default:
            cout << "\nInvalid option.\n";
    }
    return 0;
}
